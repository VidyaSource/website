---
import Page from '../../components/Page.astro'
import TutorialIndex from "../../layouts/tutorials/TutorialIndex.astro";
import {tutorials} from "../../layouts/tutorials/tutorialUtils";

const schema = {
    "@context": "https://schema.org",
    "@type": "ItemList",
    itemListElement: tutorials.map(t => t.scehma)
}

---
<Page title={"Software Development Tutorials | Vidya LLC"}
      description={"Access Vidya LLC's library of software development tutorials. Explore our free educational content covering various technologies, tools, and best practices."}
      schema={schema}>
<TutorialIndex />
</Page>